{
  "name": "@flowsterix/devtools",
  "version": "0.1.1",
  "description": "Developer tools for Flowsterix - visual element grabber for building tour steps",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kvngrf/flowsterix.git",
    "directory": "packages/devtools"
  },
  "keywords": [
    "tour",
    "onboarding",
    "devtools",
    "element-grabber",
    "flowsterix"
  ],
  "type": "module",
  "sideEffects": false,
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "default": "./dist/index.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup --config tsup.config.ts --format esm,cjs --clean && tsc --build tsconfig.json --force",
    "lint": "eslint src --ext .ts,.tsx",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18",
    "motion": ">=11"
  },
  "devDependencies": {
    "motion": "^12.29.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "peerDependenciesMeta": {
    "motion": {
      "optional": false
    }
  }
}
