{
  "name": "@flowsterix/core",
  "version": "0.4.0",
  "description": "Core engine for Flowsterix - guided tours and onboarding flows",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kvngrf/flowsterix.git",
    "directory": "packages/core"
  },
  "keywords": [
    "tour",
    "onboarding",
    "walkthrough",
    "guide",
    "flow"
  ],
  "type": "module",
