  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup --config tsup.config.ts && tsc --build tsconfig.json --force",
    "lint": "eslint src --ext .ts",
    "test": "vitest run"
  },
  "dependencies": {
    "zod": "^4.1.11"
  }
