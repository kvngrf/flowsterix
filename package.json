{
  "name": "flowsterix",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@9.12.0",
  "scripts": {
    "dev": "pnpm --filter @flowsterix/example-react-vite dev",
    "dev:react-vite": "pnpm --filter @flowsterix/example-react-vite dev",
    "dev:react-router": "pnpm --filter @flowsterix/example-react-router-vite dev",
    "dev:next": "pnpm --filter @flowsterix/example-next dev",
    "dev:shadcn": "pnpm --filter @flowsterix/shadcn-demo dev",
    "build:shadcn-registry": "pnpm --filter @flowsterix/shadcn-registry registry:build",
    "sync:shadcn-examples": "node scripts/sync-shadcn-examples.mjs",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "format": "prettier --check .",
    "format:write": "prettier --write ."
  },
  "devDependencies": {
    "@tanstack/eslint-config": "^0.3.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.2.0",
    "@types/node": "^22.10.2",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "@vitejs/plugin-react": "^5.0.4",
    "eslint": "^9.13.0",
    "jsdom": "^27.0.0",
    "prettier": "^3.5.3",
    "tailwindcss": "^4.0.6",
    "turbo": "^2.1.0",
    "tsup": "^8.0.2",
    "typescript": "^5.7.2",
    "vite": "^7.1.7",
    "vitest": "^3.0.5"
  }
}
