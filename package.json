{
  "name": "flowsterix",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@9.12.0",
  "scripts": {
    "dev": "pnpm --filter @flowsterix/example-react-vite dev",
    "dev:react-vite": "pnpm --filter @flowsterix/example-react-vite dev",
    "dev:react-router": "pnpm --filter @flowsterix/example-react-router-vite dev",
    "dev:next": "pnpm --filter @flowsterix/example-next dev",
    "dev:shadcn": "pnpm --filter @flowsterix/shadcn-demo dev",
    "dev:web": "pnpm --filter web dev",
    "dev:video": "pnpm --filter @flowsterix/video dev",
    "build:video": "pnpm --filter @flowsterix/video build",
    "build:shadcn-registry": "pnpm --filter @flowsterix/shadcn-registry registry:build",
    "sync:shadcn-examples": "node scripts/sync-shadcn-examples.mjs",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "format": "prettier --check .",
    "format:write": "prettier --write .",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "pnpm build && changeset publish"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.2",
    "@changesets/cli": "^2.29.8",
    "@playwright/test": "^1.58.0",
    "@tanstack/eslint-config": "^0.3.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.2.0",
    "@types/node": "^22.10.2",
    "@types/react": "^19.2.3",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.0.4",
    "eslint": "^9.13.0",
    "jsdom": "^27.0.0",
    "prettier": "^3.5.3",
    "tailwindcss": "^4.0.6",
    "tsup": "^8.0.2",
    "turbo": "^2.1.0",
    "typescript": "^5.7.2",
    "vite": "^7.1.7",
    "vitest": "^3.0.5"
  }
}
